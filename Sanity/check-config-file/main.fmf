summary: test if fapolicyd complains about a missing EOL in its config file
contact: Milos Malik <mmalik@redhat.com>
test: ./runtest.sh
duration: 5m
enabled: true
require:
  - sed
  - fapolicyd
  - fapolicyd-selinux
tag:
  - NoRHEL4
  - NoRHEL5
  - NoRHEL6
  - NoRHEL7
adjust+:
  - enabled: false
    when: distro == rhel-4, rhel-5, rhel-6, rhel-7
    continue: false
link:
  - verifies: https://issues.redhat.com/browse/RHEL-65625
  - verifies: https://issues.redhat.com/browse/RHEL-124283

